        <section class="Reviews" id="">
            <div class="container mt-5">
                <div class="d-flex justify-content-center row">
                    <div class="col-md-12">
                        <div class="p-3 bg-white rounded">
                            <h6>What Travelers says about us ?</h6>
        
                            <% for( let index=0; index < reviews.length; index++ ) { %>
        
        
                                <div class="review mt-4">
                                    <div class="d-flex flex-row comment-user"><img class="rounded"
                                            src="https://carnbrae.com.au/wp-content/uploads/2021/05/360_F_346839683_6nAPzbhpSkIpb8pmAwufkC7c5eD7wYws.jpg" width="40">
                                        <div class="ml-2">
                                            <div class="d-flex flex-row align-items-center"><span
                                                    class="name font-weight-bold">
                                                    <%= reviews[index].reviewName %>
                                                </span><span class="dot"></span><span class="date">
                                                    <%= reviews[index].reviewDate %>
                                                </span>
                                            </div>
                                            <div class="rating">
                                                <% for( let i=1; i<=reviews[index].rating ; i++ ) { %>
                                                    <i class="fa fa-star"></i>
                                                    <% } %>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="mt-2">
                                        <p class="comment-text">
                                            <%= reviews[index].reviewText %>
                                        </p>
                                    </div>
                                </div>
                                <% } %>
        
                        </div>
                    </div>
                </div>
            </div>
        </section>


